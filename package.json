{
    "author": "Nadim <nadim.boustani@gmail.com> (https://github.com/bnadim)",
    "name": "pretty-output",
    "description": "Library to format JSON objects into a colorful, YAML-style output. Ideal for pretty printing logs with high performance.",
    "version": "1.2.0",
    "homepage": "http://github.com/tduyng/pretty-output",
    "keywords": [
        "json",
        "javascript",
        "typescript",
        "log",
        "cli",
        "pretty",
        "formatting",
        "colors",
        "output",
        "yaml",
        "pretty-print",
        "logging",
        "performance",
        "json-format",
        "json-pretty-print"
    ],
    "maintainers": [
        {
            "name": "Ekino"
        }
    ],
    "engines": {
        "node": ">=4"
    },
    "repository": {
        "type": "git",
        "url": "http://github.com/tduyng/pretty-output.git"
    },
    "bugs": {
        "url": "http://github.com/tduyng/pretty-output/issues"
    },
    "main": "./lib/prettyoutput",
    "license": "MIT",
    "scripts": {
        "lint": "biome check --write",
        "check": "biome check",
        "build": "tsc -p tsconfig.lib.json",
        "test": "mocha 'tests/**/*.[tj]s' --reporter spec",
        "coverage": "nyc npm run test",
        "benchmark": "node ./benchmark/benchmark.js",
        "changelog": "git log $(git describe --tags --abbrev=0)..HEAD --pretty='* %s' --first-parent",
        "examples": "tsx ./examples/example.ts"
    },
    "bin": {
        "prettyoutput": "./bin/pretty_output"
    },
    "dependencies": {
        "commander": "12.x"
    },
    "devDependencies": {
        "@biomejs/biome": "1.9.4",
        "@types/mocha": "^10.0.9",
        "@types/node": "^22.7.9",
        "@types/should": "^13.0.0",
        "columnify": "^1.6.0",
        "mocha": "^10.7.3",
        "nyc": "^17.1.0",
        "prettyjson": "^1.2.5",
        "should": "^13.2.3",
        "tsx": "^4.19.1",
        "typescript": "^5.6.3"
    },
    "packageManager": "yarn@4.5.1"
}
